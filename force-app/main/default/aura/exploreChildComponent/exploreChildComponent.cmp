<aura:component >
    <aura:registerEvent name="cmpEvent" 
                        type="c:exploreComponentEvent"/>
	
    <aura:attribute name="greeting" type="String" />
    
    <lightning:input type="text" value="{! v.greeting }" />
    
    <lightning:button label="send data to Parent" 
                      onclick="{! c.sendDataToParent }" />
</aura:component>